{
  "name": "grid-styletr",
  "version": "2.0.0-12",
  "description": "Responsive React grid system built with styletron-react",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src -d dist",
    "test": "nyc ava",
    "cover": "nyc report --reporter=lcov --reporter=html"
